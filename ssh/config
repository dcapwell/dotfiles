Host *
  TCPKeepAlive              no
  ServerAliveInterval       60
  ServerAliveCountMax       10
  ControlMaster             auto
  ControlPath               /tmp/ssh_mux_%h_%p_%r
  ControlPersist            1h
Host *github*.com
  User git
  IdentityFile              ~/.ssh/github
Host ip-*
  IdentityFile              ~/.ssh/aws.pem
  PasswordAuthentication    no
  ConnectTimeout            2
  UserKnownHostsFile        /dev/null
  StrictHostKeyChecking     no
